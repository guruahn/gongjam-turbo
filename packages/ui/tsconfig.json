{
  "extends": "@gongjam/typescript-config/base.json",
  "compilerOptions": {
    "composite": false,
    "outDir": "./dist",
    "rootDir": "./src",
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "skipLibCheck": true,
    "lib": ["ESNext", "DOM"],
    "target": "ESNext",
    "module": "ESNext"
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}
